	<parameter name="no_tranksaksi" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT produk.pd_nama,transaksi_detail.trd_harga,transaksi_detail.trd_qty, transaksi_detail.trd_harga*transaksi_detail.trd_qty AS total, transaksi_header.trh_total,transaksi_header.trh_bayar, transaksi_header.trh_kembali FROM produk INNER JOIN transaksi_detail ON produk.pd_kode=transaksi_detail.trd_pd_kode INNER JOIN transaksi_header ON transaksi_detail.trd_no_transaksi=transaksi_header.trh_no_transaksi WHERE transaksi_detail.trd_no_transaksi=$P{no_tranksaksi}]]>
